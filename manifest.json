{
  "manifest_version": 2,
  "name": "CodePen Enhancement Suite",
  "description": "An opinionated collection of enhancements for CodePen.",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches":["http://codepen.io/*", "https://codepen.io/*"],
      "js":["init.js"]
    }
  ],
  "web_accessible_resources": [
    "modules/*",
    "html_partials/*",
    "styles/*"
  ]
}